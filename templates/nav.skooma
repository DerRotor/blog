local posts = require 'posts'
local i18n = require 'i18n'

local function map(tab, fn)
	local result = {}
	for key, value in ipairs(tab) do
		result[key] = fn(value)
	end
	return result
end

return function()
	return render.html(nav {
		ul(map(posts, function(post)
			return li(a {
				tostring(post.head.title);
				href = post.head.uri;
			}, " ", span {
				post.head.date;
			})
		end));
	})
end
